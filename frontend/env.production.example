# Frontend Production Environment Variables
# Copy this file to .env.production.local and update the values

# API Configuration
NEXT_PUBLIC_API_URL=http://your-ec2-ip:3000/api
NEXT_PUBLIC_APP_URL=http://your-ec2-ip:5000

# Google OAuth (if using on frontend)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id

# App Configuration
NEXT_PUBLIC_APP_NAME=Kiwi Sri Lankans Events
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=false
